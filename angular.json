{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demoFrontEnd3155Uwork4": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/custom-theme.scss",
              "src/styles.css"
            ],
            "server": "src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "demoFrontEnd3155Uwork4:build:production"
            },
            "development": {
              "buildTarget": "demoFrontEnd3155Uwork4:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "prerender": {
  "builder": "@angular-devkit/build-angular:prerender",
  "options": {
    "browserTarget": "demoFrontEnd3155Uwork4:build:production",
    "serverTarget": "demoFrontEnd3155Uwork4:server:production",
    "routes": [
      "/",
      "/inicio",
      "/usuarios",
      "/usuarios/nuevo",
      "/usuariopremios",
      "/usuariopremios/nuevo",
      "/comentarioarticulos",
      "/comentarioarticulos/nuevo",
      "/formatoarchivos",
      "/formatoarchivos/nuevo",
      "/asesorias",
      "/asesorias/nuevo",
      "/valoraciones",
      "/valoraciones/nuevo",
      "/mensajes",
      "/mensajes/nuevo",
      "/notificaciones",
      "/notificaciones/nuevo",
      "/reports",
      "/disponibilidades",
      "/disponibilidades/nuevo",
      "/archivos",
      "/archivos/nuevo",
      "/miperfil",
      "/nosotros",
      "/premios",
      "/articulos",
      "/articulos/busquedas",
      "/roles",
      "/roles/nuevo",
      "/usuarios/listarsinpassword"
    ]
  }
},
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "e25f0316-ff41-44de-98d8-ef579e8aad40"
  }
}
